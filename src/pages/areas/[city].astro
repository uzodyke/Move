const { city } = Astro.params;
const { cityData } = Astro.props;

const title = `Moving Services ${cityData.name} | Professional Movers | Tigrex Move`;
const description = `Professional moving services in ${cityData.name}. ${cityData.description} Fully insured, 24/7 availability. Get instant quote!`;

const structuredData = {
  "@context": "https://schema.org",
  "@type": ["LocalBusiness", "MovingCompany"],
  "name": `Tigrex Move - ${cityData.name}`,
  "description": `Professional moving services in ${cityData.name} and surrounding areas`,
  "telephone": "+44 7459 665 002",
  "url": `https://tigrexmove.co.uk/areas/${city}`,
  "areaServed": [
    {
      "@type": "City",
      "name": cityData.name,
      "geo": {
        "@type": "GeoCoordinates",
        "latitude": cityData.coordinates.lat,
        "longitude": cityData.coordinates.lng
      }
    }
  ],
  "address": {
    "@type": "PostalAddress",
    "streetAddress": "17 Queens Street, Portsea",
    "addressLocality": "Portsmouth",
    "postalCode": "PO1 3GB",
    "addressCountry": "GB"
  },
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": `Moving Services in ${cityData.name}`,
    "itemListElement": cityData.specialties.map(specialty => ({
      "@type": "Offer",
      "itemOffered": {
        "@type": "Service",
        "name": specialty,
        "areaServed": cityData.name
      }
    }))
  }
};

const breadcrumbStructuredData = {
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "https://tigrexmove.co.uk/"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "Areas",
      "item": "https://tigrexmove.co.uk/areas"
    },
    {
      "@type": "ListItem",
      "position": 3,
      "name": cityData.name,
      "item": `https://tigrexmove.co.uk/areas/${city}`
    }
  ]
};

const combinedStructuredData = [structuredData, breadcrumbStructuredData];
---

<BaseLayout title={title} description={description} structuredData={combinedStructuredData}>
  <Header />

  <main id="main-content">
    <!-- Breadcrumb -->
    <nav class="bg-gray-50 py-4" aria-label="Breadcrumb">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <ol class="flex items-center space-x-2 text-sm">
          <li><a href="/" class="text-gray-500 hover:text-blue-600">Home</a></li>
          <li class="text-gray-400">/</li>
          <li><a href="/areas" class="text-gray-500 hover:text-blue-600">Areas</a></li>
          <li class="text-gray-400">/</li>
          <li class="text-gray-900 font-medium">{cityData.name}</li>
        </ol>
      </div>
    </nav>

    <!-- Hero Section -->
    <section class="bg-gradient-to-br from-blue-900 to-indigo-800 text-white py-20">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 items-center">
          <div>
            <h1 class="text-4xl sm:text-5xl lg:text-6xl font-bold leading-tight mb-6">
              Professional Moving Services in {cityData.name}
            </h1>
            <p class="text-xl sm:text-2xl text-blue-100 mb-8 leading-relaxed">
              {cityData.description} Trusted by families and businesses across {cityData.name}.
            </p>

            <!-- Local Stats -->
            <div class="grid grid-cols-2 gap-6 mb-8">
              <div class="bg-white/10 rounded-lg p-4 backdrop-blur-sm">
                <div class="text-2xl font-bold mb-1">{cityData.population}</div>
                <div class="text-blue-200 text-sm">Population Served</div>
              </div>
              <div class="bg-white/10 rounded-lg p-4 backdrop-blur-sm">
                <div class="text-2xl font-bold mb-1">{cityData.areas.length}+</div>
                <div class="text-blue-200 text-sm">Areas Covered</div>
              </div>
            </div>

            <!-- CTA Buttons -->
            <div class="flex flex-col sm:flex-row gap-4">
              <a href="#quote-form" class="bg-green-600 hover:bg-green-700 text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors duration-200 text-center">
                Get Local Quote
              </a>
              <a href="tel:+447459665002" class="bg-white/10 hover:bg-white/20 backdrop-blur-sm text-white px-8 py-4 rounded-lg text-lg font-semibold transition-colors duration-200 text-center border border-white/20">
                Call: +44 7459 665 002
              </a>
            </div>
          </div>

          <!-- Quote Form -->
          <div>
            <QuoteForm client:load />
          </div>
        </div>
      </div>
    </section>

    <!-- Local Expertise -->
    <section class="py-16 bg-white">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl sm:text-4xl font-bold text-gray-900 mb-4">
            Local {cityData.name} Moving Expertise
          </h2>
          <p class="text-xl text-gray-600 max-w-3xl mx-auto">
            Our specialized knowledge of {cityData.name} ensures smooth, efficient moves throughout the area
          </p>
        </div>

        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
          {cityData.specialties.map((specialty, index) => (
            <div class="text-center" key={index}>
              <div class="w-16 h-16 bg-blue-100 rounded-full flex items-center justify-center mx-auto mb-6">
                <svg class="w-8 h-8 text-blue-600" fill="none" stroke="currentColor" viewBox="0 0 24 24">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9 12l2 2 4-4m6 2a9 9 0 11-18 0 9 9 0 0118 0z"/>
                </svg>
              </div>
              <h3 class="text-lg font-bold text-gray-900 mb-2">{specialty}</h3>
            </div>
          ))}
        </div>
      </div>
    </section>

    <!-- Areas We Cover -->
    <section class="py-16 bg-gray-50">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl sm:text-4xl font-bold text-gray-900 mb-4">
            Areas We Cover in {cityData.name}
          </h2>
          <p class="text-xl text-gray-600 max-w-3xl mx-auto">
            Professional moving services across all {cityData.name} neighborhoods and surrounding areas
          </p>
        </div>

        <div class="grid grid-cols-2 md:grid-cols-3 lg:grid-cols-4 gap-4">
          {cityData.areas.map((area, index) => (
            <div class="bg-white rounded-lg p-4 text-center shadow-sm hover:shadow-md transition-shadow duration-200" key={index}>
              <h3 class="font-semibold text-gray-900">{area}</h3>
            </div>
          ))}
        </div>

        <div class="mt-8 text-center">
          <p class="text-gray-600 mb-4">
            Don't see your area listed? We cover all of {cityData.name} and surrounding regions.
          </p>
          <a href="tel:+447459665002" class="text-blue-600 hover:text-blue-700 font-semibold">
            Call to confirm service in your area: +44 7459 665 002
          </a>
        </div>
      </div>
    </section>

    <!-- Postcodes We Serve -->
    <section class="py-16 bg-white">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl sm:text-4xl font-bold text-gray-900 mb-4">
            {cityData.name} Postcodes We Serve
          </h2>
          <p class="text-xl text-gray-600 max-w-3xl mx-auto">
            Quick reference for our service coverage across {cityData.name}
          </p>
        </div>

        <div class="flex flex-wrap justify-center gap-4 mb-8">
          {cityData.postcodes.map((postcode, index) => (
            <div class="bg-blue-100 text-blue-800 px-4 py-2 rounded-lg font-semibold" key={index}>
              {postcode}
            </div>
          ))}
        </div>

        <div class="text-center">
          <p class="text-gray-600">
            And many more postcodes throughout {cityData.name}.
            <a href="#quote-form" class="text-blue-600 hover:text-blue-700 font-semibold ml-1">Get a quote for your specific location.</a>
          </p>
        </div>
      </div>
    </section>

    <!-- Local Landmarks -->
    <section class="py-16 bg-gray-50">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="text-center mb-12">
          <h2 class="text-3xl sm:text-4xl font-bold text-gray-900 mb-4">
            Moving Near {cityData.name} Landmarks
          </h2>
          <p class="text-xl text-gray-600 max-w-3xl mx-auto">
            We know {cityData.name} inside and out, including moves near famous landmarks
          </p>
        </div>

        <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-8">
          {cityData.landmarks.map((landmark, index) => (
            <div class="text-center" key={index}>
