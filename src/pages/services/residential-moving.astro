---
import BaseLayout from '../../layouts/BaseLayout.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';
import QuoteForm from '../../components/QuoteForm';

const title = "Residential Moving Services UK | Home Moves | Tigrex Move";
const description = "Professional residential moving services across the UK. Expert home and apartment moves with full packing service. Fully insured, 24/7 availability. Get instant quote!";

const structuredData = {
  "@context": "https://schema.org",
  "@type": ["Service", "MovingCompany"],
  "name": "Residential Moving Services",
  "provider": {
    "@type": "LocalBusiness",
    "name": "Tigrex Move",
    "telephone": "+44 7459 665 002",
    "address": {
      "@type": "PostalAddress",
      "streetAddress": "17 Queens Street, Portsea",
      "addressLocality": "Portsmouth",
      "postalCode": "PO1 3GB",
      "addressCountry": "GB"
    }
  },
  "description": "Professional residential moving services including home moves, apartment relocations, packing services, and storage solutions across the UK.",
  "serviceType": "Residential Moving",
  "areaServed": [
    {"@type": "Country", "name": "United Kingdom"},
    {"@type": "City", "name": "London"},
    {"@type": "City", "name": "Manchester"},
    {"@type": "City", "name": "Birmingham"}
  ],
  "hasOfferCatalog": {
    "@type": "OfferCatalog",
    "name": "Residential Moving Services",
    "itemListElement": [
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "House Moving",
          "description": "Complete house relocation services"
        }
      },
      {
        "@type": "Offer",
        "itemOffered": {
          "@type": "Service",
          "name": "Apartment Moving",
          "description": "Flat and apartment moving services"
        }
      }
    ]
  }
};

const breadcrumbStructuredData = {
  "@context": "https://schema.org",
  "@type": "BreadcrumbList",
  "itemListElement": [
    {
      "@type": "ListItem",
      "position": 1,
      "name": "Home",
      "item": "https://tigrexmove.co.uk/"
    },
    {
      "@type": "ListItem",
      "position": 2,
      "name": "Services",
      "item": "https://tigrexmove.co.uk/services"
    },
    {
      "@type": "ListItem",
      "position": 3,
      "name": "Residential Moving",
      "item": "https://tigrexmove.co.uk/services/residential-moving"
    }
  ]
};

const combinedStructuredData = [structuredData, breadcrumbStructuredData];
---

<BaseLayout title={title} description={description} structuredData={combinedStructuredData}>
  <Header />

  <main id="main-content">
    <!-- Breadcrumb -->
    <nav class="bg-gray-50 py-4" aria-label="Breadcrumb">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <ol class="flex items-center space-x-2 text-sm">
          <li><a href="/" class="text-gray-500 hover:text-blue-600">Home</a></li>
          <li class="text-gray-400">/</li>
          <li><a href="/services" class="text-gray-500 hover:text-blue-600">Services</a></li>
          <li class="text-gray-400">/</li>
          <li class="text-gray-900 font-medium">Residential Moving</li>
        </ol>
      </div>
    </nav>

    <!-- Hero Section -->
    <section class="bg-gradient-to-br from-blue-900 to-indigo-800 text-white py-20">
      <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
        <div class="grid grid-cols-1 lg:grid-cols-2 gap-12 items-center">
          <div>
            <h1 class="text-4xl sm:text-5xl lg:text-6xl font-bold leading-tight mb-6">
              Professional Residential Moving Services
